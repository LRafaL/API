				{
					"name": "Product search by category",
					"item": [
						{
							"name": "Search for products by category Watches",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"// Проверка, что статус ответа 200\r",
											"pm.test(\"Статус ответа 200\", function () {\r",
											"    pm.response.to.have.status(200);\r",
											"});\r",
											"\r",
											"// Парсинг ответа\r",
											"var jsonData = pm.response.json();\r",
											"\r",
											"// Проверка, что ответ является массивом\r",
											"pm.test(\"Ответ является массивом\", function () {\r",
											"    pm.expect(jsonData).to.be.an('array');\r",
											"});\r",
											"\r",
											"// Проверка каждого продукта в массиве\r",
											"jsonData.forEach(function(product) {\r",
											"    // Проверка наличия обязательных полей\r",
											"    pm.test(\"Продукт \" + product.product_id + \" имеет обязательные поля\", function () {\r",
											"        pm.expect(product).to.have.property('product_id');\r",
											"        pm.expect(product).to.have.property('name');\r",
											"        pm.expect(product).to.have.property('description');\r",
											"        pm.expect(product).to.have.property('price');\r",
											"        pm.expect(product).to.have.property('category');\r",
											"        pm.expect(product).to.have.property('manufacturer');\r",
											"        pm.expect(product).to.have.property('imageUrl');\r",
											"        pm.expect(product).to.have.property('freeShipping');\r",
											"    });\r",
											"\r",
											"    // Проверка типа данных для каждого поля\r",
											"    pm.test(\"Продукт \" + product.product_id + \" имеет правильные типы данных\", function () {\r",
											"        pm.expect(product.product_id).to.be.a('number');\r",
											"        pm.expect(product.name).to.be.a('string');\r",
											"        pm.expect(product.description).to.be.a('string');\r",
											"        pm.expect(product.price).to.match(/^\\d+(\\.\\d{1,2})?$/); // проверка формата цены\r",
											"        pm.expect(product.category).to.be.a('string');\r",
											"        pm.expect(product.manufacturer).to.be.a('string');\r",
											"        pm.expect(product.imageUrl).to.be.a('string');\r",
											"        pm.expect(product.freeShipping).to.be.oneOf([0, 1]); // проверка булевого значения в виде 0 или 1\r",
											"    });\r",
											"\r",
											"    // Дополнительные проверки для каждого продукта\r",
											"    pm.test(\"Продукт \" + product.product_id + \" имеет допустимое значение freeShipping\", function () {\r",
											"        pm.expect([0, 1]).to.include(product.freeShipping);\r",
											"    });\r",
											"\r",
											"    pm.test(\"Продукт \" + product.product_id + \" имеет правильный формат imageUrl\", function () {\r",
											"        pm.expect(product.imageUrl).to.match(/^https?:\\/\\/|^\\/images\\/[\\w\\-]+\\.(jpg|jpeg|png|webp)$/);\r",
											"    });\r",
											"});\r",
											"\r",
											"// Дополнительные проверки для первого продукта\r",
											"if (jsonData.length > 0) {\r",
											"    var firstProduct = jsonData[0];\r",
											"\r",
											"    pm.test(\"Первый продукт имеет ID 2\", function () {\r",
											"        pm.expect(firstProduct.product_id).to.eql(2);\r",
											"    });\r",
											"\r",
											"    pm.test(\"Первый продукт называется 'Samsung Active 5'\", function () {\r",
											"        pm.expect(firstProduct.name).to.eql(\"Samsung Active 5\");\r",
											"    });\r",
											"}"
										],
										"type": "text/javascript",
										"packages": {}
									}
								}
							],
							"request": {
								"method": "GET",
								"header": [
									{
										"key": "Accept",
										"value": "application/json"
